{
  "expo": {
    "name": "Snow Pet",
    "slug": "SnowPet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "snowpet",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#00AEEF",
          "image": "./assets/images/splash.png",
          "resizeMode": "cover"
        }
      ],
      "expo-router",
      "expo-sqlite",
      "expo-font",
      "expo-web-browser"
    ],
    "android": {
      "package": "com.autentikadigital.snowpet",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#00AEEF"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~3347511713"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aa0d21d1-6c9b-43eb-8172-eb1657d99c41"
      }
    }
  }
}
